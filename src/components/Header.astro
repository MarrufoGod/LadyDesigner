console.log('javascript')
---
<header class="py-8 px-4 mx-auto max-w-xl lg:py-16 lg:px-6">
    <div class="text-center">
        <!-- Logos -->
        <img src="/lobo.png" alt="Logo" class="w-40 h-auto mx-auto mb-4">
        <img src="/encabezado.png" alt="Encabezado" class="w-60 h-auto mx-auto mb-8">

        <!-- Contenedor de botones en línea -->
        <div class="flex flex-row justify-center gap-7">
            <!-- Botón Informacion -->
            <a class="group relative inline-flex items-center justify-center border border-red-600 px-15 py-3 focus:ring-3 focus:outline-none overflow-hidden" href="/">
                <span class="absolute inset-y-0 right-0 w-[2px] bg-red-600 transition-all group-hover:w-full"></span>
                <span class="relative text-sm font-medium text-red-600 transition-colors group-hover:text-white">Información</span>
            </a>
        
            <!-- Botón Flyers -->
            <a class="group relative inline-flex items-center justify-center border border-red-600 px-15 py-3 focus:ring-3 focus:outline-none overflow-hidden" href="/Flyers">
                <span class="absolute inset-x-0 bottom-0 h-[2px] bg-red-600 transition-all group-hover:h-full"></span>
                <span class="relative text-sm font-medium text-red-600 transition-colors group-hover:text-white">Flyers</span>
            </a>
        
            <!-- Botón Manuales -->
            <a class="group relative inline-flex items-center justify-center border border-red-600 px-15 py-3 focus:ring-3 focus:outline-none overflow-hidden" href="#">
                <span class="absolute inset-x-0 top-0 h-[2px] bg-red-600 transition-all group-hover:h-full"></span>
                <span class="relative text-sm font-medium text-red-600 transition-colors group-hover:text-white">Manuales</span>
            </a>
        
            <!-- Botón Logos -->
            <a class="group relative inline-flex items-center justify-center border border-red-600 px-15 py-3 focus:ring-3 focus:outline-none overflow-hidden" href="/Logo">
                <span class="absolute inset-y-0 left-0 w-[2px] bg-red-600 transition-all group-hover:w-full"></span>
                <span class="relative text-sm font-medium text-red-600 transition-colors group-hover:text-white">Logos</span>
            </a>
        
            <!-- Botón Tarjetas -->
            <a class="group relative inline-flex items-center justify-center border border-red-600 px-15 py-3 focus:ring-3 focus:outline-none overflow-hidden" href="/Tarjetas">
                <span class="absolute inset-x-0 top-0 h-[2px] bg-red-600 transition-all group-hover:h-full"></span>
                <span class="relative text-sm font-medium text-red-600 transition-colors group-hover:text-white">Tarjetas</span>
            </a>
        
            <!-- Botón Publicidad -->
            <a class="group relative inline-flex items-center justify-center border border-red-600 px-15 py-3 focus:ring-3 focus:outline-none overflow-hidden" href="/Publicidad">
                <span class="absolute inset-y-0 left-0 w-[2px] bg-red-600 transition-all group-hover:w-full"></span>
                <span class="relative text-sm font-medium text-red-600 transition-colors group-hover:text-white">Publicidad</span>
            </a>
        
            <!-- Botón Impresos -->
            <a class="group relative inline-flex items-center justify-center border border-red-600 px-15 py-3 focus:ring-3 focus:outline-none overflow-hidden" href="/Impresos">
                <span class="absolute inset-x-0 bottom-0 h-[2px] bg-red-600 transition-all group-hover:h-full"></span>
                <span class="relative text-sm font-medium text-red-600 transition-colors group-hover:text-white">Impresos</span>
            </a>
        </div>
        
    </div>
</header>


